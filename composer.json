{
    "name": "atefrihane/livewire-password-meter",
    "description": "A package dedicated to check password strength",
    "keywords": [
        ":vendor_name",
        "laravel",
        ":package_slug"
    ],
    "homepage": "https://github.com/:vendor_slug/:package_slug",
    "license": "MIT",
    "authors": [
        {
            "name": ":Atef Rihane",
            "email": "rihaneatef@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "bjeavons/zxcvbn-php": "^1.3",
        "illuminate/contracts": "^9.0",
        "livewire/livewire": "^2.11",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Atef\\LivewirePasswordMeter\\": "src",
            "Atef\\LivewirePasswordMeter\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atef\\LivewirePasswordMeter\\Tests\\": "tests"
        }
    },
    "scripts": {
      
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit"
   
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Atef\\LivewirePasswordMeter\\SkeletonServiceProvider"
            ],
            "aliases": {
                "LivewirePasswordMeter": "Atef\\LivewirePasswordMeter\\Facades\\Skeleton"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
